{
    "name": "omatamix/session-lock",
    "description": "Securely manage and preserve session data.",
    "homepage": "https://omatamix.github.io/session-lock/",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "cache/simple-cache-bridge": "^1.1",
        "psr/cache": "^1.0|^2.0|^3.0",
        "psr/simple-cache": "^1.0",
        "roave/dont": "^1.3",
        "symfony/options-resolver": "^5.2",
        "symfony/polyfill-apcu": "^1.22",
        "symfony/polyfill-ctype": "^1.22"
    },
    "suggest": {
        "defuse/php-encryption": "To use the `defuse` encryption adapter (OpenSSL).",
        "paragonie/halite": "To use the `halite` encryption adapter (Sodium)."
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Omatamix\\SessionLock\\": "./src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
